{
  "name": "postit",
  "version": "1.0.0",
  "description": "An application that allows friends and colleagues create groups for notifications",
  "scripts": {
    "start": "node index.js",
    "start-dev": "NODE_ENV=development nodemon --exec babel-node -- index.js",
    "bundle-assets": "webpack --config=webpack.config.prod.js",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha ./test/usertests.js ./test/grouptests.js --compilers,js:babel-core/register",
    "client-test": "jest --coverage",
    "client-test:watch": "npm run client-test -- --watch",
    "postinstall": "npm run bundle-assets && npm run migrate-production",
    "gulp-test": "NODE_ENV=test gulp test --timeout 15000",
    "migrate-test": "NODE_ENV=test sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migrate-production": "NODE_ENV=production sequelize db:migrate"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/client/tests/test-env.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3m3kalionel/PostIt.git"
  },
  "author": "Emeka Lionel Okoro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/3m3kalionel/PostIt/issues"
  },
  "homepage": "https://github.com/3m3kalionel/PostIt#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "gulp-babel": "^6.1.2",
    "gulp-babel-istanbul": "^1.6.0",
    "gulp-inject-modules": "^1.0.0",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "isparta": "^4.0.0",
    "jest": "^20.0.4",
    "jquery": "^3.2.1",
