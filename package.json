{
  "name": "postit",
  "version": "1.0.0",
  "description": "An application that allows friends and colleagues create groups for notifications",
  "scripts": {
    "start": "node index.js",
    "start-dev": "NODE_ENV=development nodemon --exec babel-node -- index.js",
    "bundle-assets": "webpack --config=webpack.config.prod.js",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha ./test/usertests.js ./test/grouptests.js --compilers js:babel-core/register",
